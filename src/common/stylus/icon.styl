@font-face
  font-family: 'mysell-icon'
  src:  url('../fonts/mysell-icon.eot?a8vzit')
  src:  url('../fonts/mysell-icon.eot?a8vzit#iefix') format('embedded-opentype'),
    url('../fonts/mysell-icon.ttf?a8vzit') format('truetype'),
    url('../fonts/mysell-icon.woff?a8vzit') format('woff'),
    url('../fonts/mysell-icon.svg?a8vzit#mysell-icon') format('svg')
  font-weight: normal
  font-style: normal


[class^="h-"], [class*=" h-"]
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'mysell-icon' !important
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale


